{
  "name": "@crossmarkio/sdk",
  "version": "0.3.9",
  "repository": "https://github.com/crossmarkio/sdk.git",
  "author": "interc0der <intercoder@crossmark.io>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "sdk",
    "crossmark",
    "XRPL",
    "wallet"
  ],
  "files": [
    "pack"
  ],
  "main": "./pack/cjs/index.js",
  "types": "./pack/cjs/index.d.ts",
  "browser": "./pack/umd/index.js",
  "unpkg": "./pack/umd/index.js",
  "jsdelivr": "./pack/umd/index.js",
  "exports": {
    ".": {
      "import": "./pack/umd/index.js",
      "require": "./pack/cjs/index.js",
      "browser": {
        "default": "./pack/umd/index.js"
      },
      "module": {
        "default": "./pack/umd/index.js"
      },
      "default": "./pack/cjs/index.js"
    },
    "./cjs": {
      "default": "./pack/cjs/index.js"
    },
    "./umd": {
      "default": "./pack/umd/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./pack/cjs/index.d.ts"
      ],
      "cjs": [
        "./pack/cjs/index.d.ts"
      ],
      "umd": [
        "./pack/umd/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=18.12"
  },
  "scripts": {
    "env": "dotenv-run",
    "env:prod": "dotenv-run -e production",
    "-----": "",
    "ts-node": "ts-node -P ./tsconfig.test.json",
    "-------------": "",
    "dev": "yarn rebuild:watch",
    "start": "yarn rebuild:watch",
    "build": "yarn prebuild && yarn c && yarn postbuild",
    "------------": "",
    "c": "tsc && tsc-alias",
    "rebuild": "yarn c && yarn postbuild",
    "rebuild:watch": "yarn rebuild && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "prebuild": "rm -rf ./dist && rm -rf ./build && rm -rf ./compiled",
    "postbuild": "yarn ts-node ./scripts/build.ts",
    "build:watch": "yarn build && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "-----------": "",
    "dist": "yarn webpack && yarn rollup",
    "rollup": "rollup -c ./config/rollup.config.mjs",
    "webpack": "npx webpack -c ./config/webpack.config.js",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier 'src' --write --list-different",
    "format:check": "prettier 'src' --check",
    "-------": "",
    "test": "yarn env -- jest --all --coverage --forceExit",
    "testOne": "yarn env -- jest --coverage --forceExit",
    "----------": "",
    "pub": "cd ./dist && npm publish",
    "pub:beta": "npm publish --tag beta",
    "pub:next": "npm publish --tag next",
    "pub:release": "npm publish --access public"
  },
  "devDependencies": {
    "@dotenv-run/cli": "^1.3.5",
    "@monorepo/configs": "workspace:^",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/fs-extra": "^11",
    "@types/jest": "^29.5.12",
    "archiver": "^6.0.1",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "crypto-browserify": "^3.12.0",
    "fs-extra": "^11.2.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.3.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url": "^0.11.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "events": "^3.3.0",
    "@crossmarkio/typings": "0.0.5"
  }
}