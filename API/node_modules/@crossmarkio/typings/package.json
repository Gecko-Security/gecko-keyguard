{
  "name": "@crossmarkio/typings",
  "version": "0.0.5",
  "author": "interc0der <intercoder@crossmark.io>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "monorepo",
    "typings",
    "crossmark",
    "XRPL",
    "wallet"
  ],
  "files": [
    "build"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": {
      "default": "./build/src/index.js"
    },
    "./sdk": {
      "default": "./build/src/sdk/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./build/src/index.d.ts"
      ],
      "sdk": [
        "./build/src/sdk/index.d.ts"
      ]
    }
  },
  "scripts": {
    "ts-node": "ts-node -P ./tsconfig.test.json",
    "-----------": "",
    "dev": "yarn rebuild:watch",
    "build": "yarn prebuild && yarn c && yarn postbuild",
    "----------": "",
    "prebuild": "rm -rf ./dist && rm -rf ./build && rm -rf ./compiled",
    "postbuild": "mkdir ./dist && yarn ts-node ./scripts/prepare.ts",
    "--------": "",
    "build:watch": "yarn build && yarn watch",
    "rebuild": "yarn c && yarn postbuild",
    "rebuild:watch": "yarn rebuild && yarn watch",
    "watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "c": "tsc && tsc-alias",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier 'src' --write --list-different",
    "format:check": "prettier 'src' --check",
    "---------": "",
    "test": "jest --all --forceExit",
    "testOne": "jest --forceExit",
    "------------": "",
    "pub": "cd ./dist && npm publish",
    "pub:beta": "npm publish --tag beta",
    "pub:next": "npm publish --tag next",
    "pub:release": "npm publish --access public"
  },
  "devDependencies": {
    "@monorepo/configs": "workspace:^",
    "@types/node-forge": "^1.3.10",
    "archiver": "^6.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@transia/xrpl": "^2.7.3-alpha.24",
    "@types/chrome": "^0.0.253",
    "node-forge": "^1.3.1",
    "xrpl": "2.14.1"
  }
}